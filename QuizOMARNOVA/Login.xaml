<Page x:Class="QuizOMARNOVA.Login"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:QuizOMARNOVA"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Login">

    <Grid HorizontalAlignment="Center" Height="325" Margin="0,65,0,0" VerticalAlignment="Top" Width="800" Background="#FFFFCDCD">
        <Label Content="LOGIN" HorizontalAlignment="Center" VerticalAlignment="Top" FontSize="36"/>
        <Label Content="Username" HorizontalAlignment="Left" Margin="276,93,0,0" VerticalAlignment="Top"/>
        <Label Content="Password" HorizontalAlignment="Left" Margin="278,152,0,0" VerticalAlignment="Top"/>
        <TextBox Name="txtUsername" HorizontalAlignment="Left" Height="21" Margin="352,98,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top" Width="150"/>
        <PasswordBox Name="txtPassword" HorizontalAlignment="Left" Height="21" Margin="352,154,0,0" VerticalAlignment="Top" Width="150"/>
        <Button Name="btnLogin" Content="Log in" HorizontalAlignment="Left" Height="20" Margin="340,202,0,0" VerticalAlignment="Top" Width="114" Click="btnLogin_Click"/>
    </Grid>
</Page>
